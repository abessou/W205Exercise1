using exercise1;

CREATE TABLE transformed_hospitals AS
SELECT TRANSFORM
(PROVIDERID, HOSPITALNAME, ADDRESS, CITY, STATE, ZIPCODE, COUNTYNAME, 
PHONENUMBER, HOSPITALTYPE, HOSPITALOWNERSHIP, EMERGENCYSERVICES)
USING 'transform_hospitals.py' AS
(ProviderId string, Name string, Address string, State string, Zip string,
County string, Phone string, Type string, Ownership string, 
EmergencyServices string)
FROM hospitals;
